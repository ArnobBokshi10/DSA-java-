public class Main {
    public static void numericalrectagular(int n){
        for (int i = 1 ; i <= n; i++){
            for(int j = i ; j<= n ; j++){
                System.out.print(j);
            }
            for(int k = 1 ; k <= i-1;k++){
                System.out.print(k);
            }System.out.println();
        }
    }
    public static void main(String[] args) {
        numericalrectagular(7);    
    }
}
