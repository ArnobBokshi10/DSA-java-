public class Main
{
    public static void main(String[] args) 
    {
        int i,j;
        int row = 5;
        for(i=1;i<=row;i++)
        {
            for(j=i;j<=row;j++)
                System.out.print(" ");
            for(j=1;j<=i;j++)
                System.out.print(j+"");
            for(j=i-1;j>=1;j--)
                System.out.print(j+"");
            System.out.println();
         }
    }
}
