public class Main
{
    public static void starvoidPlus(int n ){
     for(int i = 1; i <=n ; i++){
         for(int j = 1; j<=n;j++){
             if( i == (n/2+1)||j==(n/2+1)){
             System.out.print(" ");
         }
         else{
             System.out.print("*"+" ");
         }
     }System.out.println();
    }
    }
    public static void main(String[] args) 
    {
        starvoidPlus(5);
      }
    }
